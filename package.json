{
  "name": "gsfm",
  "version": "0.2.8",
  "private": true,
  "scripts": {
    "dev:next": "next dev --turbopack",
    "dev": "concurrently npm:dev:*",
    "prebuild": "npm run codegen",
    "build": "NEXT_PUBLIC_URL=https://gsfm.maayanlab.cloud next build",
    "start": "next start",
    "lint": "next lint",
    "codegen:kysely": "kysely-codegen",
    "codegen:trpc": "ts-node src/lib/trpc/codegen.ts",
    "codegen": "concurrently npm:codegen:*",
    "version": "npm run build && ts-node src/cli/version-compose.ts && git add docker-compose.yaml && docker compose build gsfm-app && docker compose push gsfm-app"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.9",
    "@trpc/client": "^11.0.0-rc.808",
    "@trpc/next": "^11.0.0-rc.808",
    "@trpc/react-query": "^11.0.0-rc.808",
    "@trpc/server": "^11.0.0-rc.808",
    "@trpc/tanstack-react-query": "^11.0.0-rc.808",
    "classnames": "^2.5.1",
    "glob": "^11.0.1",
    "immutable": "^5.0.3",
    "kysely": "^0.27.5",
    "kysely-codegen": "^0.17.0",
    "next": "15.1.7",
    "pg": "^8.13.3",
    "pg-cursor": "^2.12.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/pg": "^8.11.11",
    "@types/pg-cursor": "^2.7.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "daisyui": "^5.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "kysely-ctl": "^0.11.1",
    "postcss": "^8",
    "tailwindcss": "^4.0.10",
    "typescript": "^5"
  }
}
